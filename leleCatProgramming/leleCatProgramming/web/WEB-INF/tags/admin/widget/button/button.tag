<%@ tag description="" pageEncoding="UTF-8" %>

<%@ attribute name="name"%>
<%@ attribute name="icon"%>
<%@ attribute name="class_name"%>
<%@ attribute name="onclick"%>
<%@ attribute name="height"%>

<button class="btn btn-icon ${class_name}" onclick="${onclick}" style="width: ${height};">
	<i class="${icon}"></i>
	<span>${name}</span>
</button>
